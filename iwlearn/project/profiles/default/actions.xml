<?xml version="1.0"?>
<object name="portal_actions">
  <object name="object_buttons">
    <object name="harvest_gef_online" meta_type="CMF Action">
      <property name="title">Harvest GEF online</property>
      <property name="description"/>
      <property name="url_expr">string:${object_url}/gefonlineharvest_view</property>
      <property name="icon_expr"/>
      <property name="available_expr">python:object.meta_type in ('Project Database')</property>
      <property name="permissions">
        <element value="Modify portal content"/>
      </property>
      <property name="visible">True</property>
    </object>
    <object name="update_from_gef_online" meta_type="CMF Action">
      <property name="title">Update from GEF online</property>
      <property name="description"/>
      <property name="url_expr">string:${object_url}/gefonlineupdate_view</property>
      <property name="icon_expr"/>
      <property name="available_expr">python:object.meta_type in ('Project Database')</property>
      <property name="permissions">
        <element value="Modify portal content"/>
      </property>
      <property name="visible">True</property>
    </object>
  </object>
</object>
